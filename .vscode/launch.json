{
	"configurations": [
		{
			"name": "Application [Firefox]",
			"reAttach": true,
			"request": "launch",
			"type": "firefox",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/app"
		},
		{
			"name": "Application [Chrome]",
			"request": "launch",
			"type": "chrome",
			"url": "http://localhost:5173",
			"webRoot": "${workspaceFolder}/app"
		},
		{
			"cwd": "${workspaceFolder}/server",
			"debugAdapter": "legacy",
			"host": "127.0.0.1",
			"mode": "remote",
			"name": "Server",
			"port": 4000,
			"remotePath": "/sfs/server",
			"request": "attach",
			"type": "go"
		}
	],
	"version": "0.2.0"
}
