{
  "configurations": [
    {
      "name": "Launch Application",
      "request": "launch",
      "type": "firefox",
      "url": "http://localhost",
      "webRoot": "${workspaceFolder}/app"
    },
    {
      "host": "localhost",
      "mode": "remote",
      "name": "Attach Server",
      "port": 4000,
      "request": "attach",
      "substitutePath": [
        {
          "from": "${workspaceFolder}/server",
          "to": "/sfs/server"
        }
      ],
      "type": "go"
    }
  ]
}
